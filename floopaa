	open.TextColor3 = Color3.new(0, 0, 0)
	open.TextSize = 14
	open.MouseButton1Down:connect(function()
		openmain.Visible = false
		main.Visible = true
	end)

	main.Name = "main"
	main.Parent = PrisonBreakerv15
	main.BackgroundColor3 = Color3.new(0, 1, 0)
