		c.Archivable = true
		Rig = c:Clone()
		Rig.Parent = c
		Rig.Name = "Rig"

		for i,v in pairs(c.Rig:GetChildren()) do
			if v:IsA("BasePart") then 
				v.Transparency = 1
			end
		end
		for _,acc in pairs(c.Rig:GetChildren()) do
			if acc:IsA("Accessory") then 
				acc.Handle.Transparency = 1
			end
		end
